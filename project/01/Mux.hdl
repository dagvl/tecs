/** 
 * Multiplexor.  If sel=0 then out = a else out = b.
 */

CHIP Mux {

    IN  a, b, sel;
    OUT out;

    PARTS:
    Not (in=sel, out=notsel);
		And (a=a, b=notsel, out=parta);
		And (a=b, b=sel, out=partb);
		Or (a=parta, b=partb, out=out);
}
