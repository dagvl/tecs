/**
 *  Exclusive-or gate.  out = a xor b.
 */

CHIP Xor {

    IN  a, b;
    OUT out;

    PARTS:
    Not(in=a, out=nota);
		Not(in=b, out=notb);
		And(a=a, b=notb, out=left);
		And(a=nota, b=b, out=right);
		Or(a=left, b=right, out=out);
}
